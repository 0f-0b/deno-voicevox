{
  "name": "@ud2/deno-voicevox",
  "exports": "./mod.ts",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.0"
  },
  "compilerOptions": {
    "exactOptionalPropertyTypes": true
  },
  "publish": {
    "exclude": [
      ".gitattributes",
      ".github",
      "deno.lock",
      "**/test.ts",
      "**/*_test.ts",
      "**/*_wrapper.cmd"
    ]
  },
  "tasks": {
    "cache": "deno cache --allow-import=jsr.io:443 examples/simple_tts.ts",
    "lint": "deno fmt --check && deno lint && deno check --all --allow-import=jsr.io:443 examples/simple_tts.ts",
    "test": "deno test --check=all --allow-import=jsr.io:443 --allow-read --allow-write --allow-run=./examples/simple_tts.ts,./examples/simple_tts_wrapper.cmd --allow-ffi"
  }
}
